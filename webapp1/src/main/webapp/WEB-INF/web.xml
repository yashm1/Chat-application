<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>com.mycompany.servlets.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>signUp</servlet-name>
        <servlet-class>com.mycompany.servlets.signUp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>chatstore</servlet-name>
        <servlet-class>com.mycompany.servlets.chatstore</servlet-class>
    </servlet>
     <servlet>
        <servlet-name>reload</servlet-name>
        <servlet-class>com.mycompany.servlets.reload</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>signUp</servlet-name>
        <url-pattern>/signUp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>chatstore</servlet-name>
        <url-pattern>/chatstore</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>reload</servlet-name>
        <url-pattern>/reload</url-pattern>
    </servlet-mapping>
 
  
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
